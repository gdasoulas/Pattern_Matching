function [Classes_voice]=class_divide(samples)
%Diaxwrismos twn 9 klasewn

    Classes_voice{8}=samples(1:14);
    Classes_voice{5}=samples(15:29);
    Classes_voice{4}=samples(30:44);
    Classes_voice{9}=samples(45:59);
    Classes_voice{1}=samples(60:74);
    Classes_voice{7}=samples(75:89);
    Classes_voice{6}=samples(90:103);
    Classes_voice{3}=samples(104:118);
    Classes_voice{2}=samples(119:133);
    
    for i=1:9
        for j=1:size(Classes_voice{i})
            tmp=cell(1);
            tmp{1}= Classes_voice{i}(j).mffc_fframes;
            Classes_voice{i}(j) = tmp
end

